{
  "expo": {
    "name": "farmit-mobile",
    "slug": "farmit-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": { "image": "./assets/splash-icon.png", "resizeMode": "contain", "backgroundColor": "#0b1220" },
    "ios": { "supportsTablet": true },
    "android": { "adaptiveIcon": { "foregroundImage": "./assets/adaptive-icon.png", "backgroundColor": "#0b1220" }, "edgeToEdgeEnabled": true },
    "web": { "favicon": "./assets/favicon.png" },
    "plugins": [["expo-router", { "origin": "index.js" }]]
  }
}
